body
{
  font-family: 'Roboto', sans-serif;
}
.cycle-slideshow{
	position: relative;
}
.margin-bottom-40{
	margin-bottom:40px;
}
.margin-bottom-60{
	margin-bottom:60px;
}
.text-uppercase{
	text-transform: uppercase;
}
header
{
  background-color:#ffffff;
  min-height:103px;
  
  .logo
  {
    margin-top:14px;
  }
	.menu{
		list-style:none;
		margin-bottom:0px;
		li{
			display: inline-block;
			position:relative;
			padding:0px 15px;
			min-height: 103px;
			a{
				margin-top:40px;
				display: block;
        font-size:14px;
        color:#1ab7ea;
        text-transform:uppercase;
				text-decoration:none;
			}
			
			ul{
				
				position:absolute;
				top:103px;
				z-index: 10000;
				background-color: #1ab7ea;
    		margin: 0px;
    		padding: 0px;
				display:none;
				left: 0px;
				
				li{
					display:block;
					width:300px;
					min-height: 0px;
					text-align: left;
					a{		
						padding: 10px 0px;
						margin: 0px;
						margin-top: 1px;			
						color:#ffffff;
						font-weight: normal;
					}
				}
				li:hover{
					background-color: #ffffff;
					a{
						color:#1ab7ea;
					}	
				}
			}
			
		}	
		li:hover
		{
			background-color:#1ab7ea;

			a{
				color:#ffffff;
			}
			ul{
				display:block;
			}
		}
	}
  #minimenu{

		margin-bottom: 0px;
		margin-top: 15px;
		
		.nav-bar{
			margin: 0px 0px;
		}
		
		li{
			border-bottom: 1px solid #c6c6c6;
			padding-top: 15px;
			padding-bottom: 15px;
		}
	}
}

section.highlight
{
  .fixed-highlight{
		position: relative;
		left: 50%;
		-webkit-transform: translateX(-50%);
		-moz-transform:translateX(-50%);
		-ms-transform: translateX(-50%);
		-o-transform: translateX(-50%);				
		transform: translateX(-50%);	
	}
  #slides
  { 
	width: auto !important;
	max-width: 960px;
    margin:auto;
  }
  #nav-pages
  {
	z-index: 5000;
	position: relative;
	top: -25px;
	width: 100%;	
	height: 8px;
	text-align: center;
  }
	#nav-pages a
	{
		width:8px;
		height:8px;
		background-image:url('/assets/btn-cycle.png');
		text-decoration: none;
		display: inline-block;
		margin: 0px 2px
	}                         
	
	#nav-pages a.activeSlide
	{
		background-image:url('/assets/btn-cycle-active.png'); 
	}
	.show_text_highlight		
	{
		position:absolute;
		top: 50%;
		left:50%;
		-webkit-transform: translate(-50%,-50%);
		-ms-transform: translate(-50%,-50%);
		-moz-transform: translate(-50%,-50%);
		-o-transform: translate(-50%,-50%);
		transform: translate(-50%,-50%);
		/*margin-left: -585px;*/
		p.text-highlight
		{
			/*margin-top: 125px;*/
			color:#ffffff;
			font-size:38px;
			
		}	
		.what_we_do{
			font-size:2em !important;
		}
		p.text-highlight.subtitle
		{
			margin-top: 15px;
			font-size:18px;
		}


		
		
	}
	
  
}

section.highlight-thumb
{
	.container-fluid{
		background-color:#1ab7ea;
	}
	.show_thumb_box{
		float:left;
		height: 180px;
		width:193px;
		color:#b3def7;
		font-size:12px;
		text-transform: uppercase;
		padding: 25px 15px 0px 15px !important;
		p{
			display: table-cell;
			vertical-align: middle;
			height:80px;
			width:163px;
			font-size: 13px;
			font-weight: 900;
		}
	}
		.img_thumbnail_shadow{
			display:none;
		}
		.cycle-pager-active{
			background-color: #b3def7;
			color:#1ab7ea;
		}
		.cycle-pager-active .img_thumbnail_shadow{
			display:block;
		}
		.cycle-pager-active .img_thumbnail{
			display:none;
		}

	
}
section.noticias
{
  padding:70px 0px 70px 0px;
  background-color:#e7e7e8;
  position:relative; 
  
  form
  {
    .input-busca
    {
      font-size:18px;
      color:#cbcbcb;
      border:none;
      border-radius: 0px;
      box-shadow: inset 0 0px 0px rgba(0,0,0,0);
      height:60px;
      
      background-color:#f9f9f9;
      background-image:url("/assets/ico-lupa.jpg");
      background-repeat:no-repeat;
      background-position:right center;
    }
  }
  
  #CleanSearch
  {
    font-size:12px;
    background-color:transparent;
    border:none;
    text-decoration:underline;
    color:#569ec7;
  }
  
  .article_show{
      min-height:160px;
  }
  .image_box{
		padding-right: 0px;
	}
  .items
  {
    padding:30px 0px 30px 0px;
    
    h2
    {
      font-size:24px;
      color:#1ab7ea;
    }
    a
    {
      font-size:12px;
      color:#6c6c6c;
    }
    
    h2
    {
      display:inline;
    }  
  }
	#paginate{
		text-align: center;
		border-bottom: 1px solid #6c6c6c;
		border-top: 1px solid #6c6c6c;
		font-size:18px;
		.pagination{
			a{
				color:#6c6c6c;
				margin: 0px 13px;
				text-decoration:none;
			}
			.current{
				color:#1ab7ea;
				margin: 0px 13px;
			}
			.disabled{
				color:#b6b6b6;
			}
			a, .current{
				cursor:pointer;
			}
		}
	}
}

