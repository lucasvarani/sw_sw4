  blockquote {
    margin: 0.25em 0;
    padding: 0.25em 40px;
    line-height: 1;
    position: relative;
    border:none;
    text-align:center;
  }
  blockquote p{
    font-size: 18px;
    font-style: italic;
    color: #96d6fb;
  }
  
  blockquote:before {
    display: block;
    content: "\201C";
    font-size: 80px;
    position: absolute;
    left: 30px;
    top: -20px;
    color: #668790;
  }
  
  blockquote cite {
    color: #00ade9;
    font-size: 14px;
    display: block;
    margin-top: 5px;
  }

  blockquote cite:before {
  content: "\2014 \2009";
  } 

  blockquote:after {
    display: block;
    content: "\201D";
    font-size: 80px;
    position: absolute;
	right: 30px;
    color: #668790;
  }

